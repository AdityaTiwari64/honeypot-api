{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "env": {
        "HONEYPOT_API_KEY": "@honeypot_api_key",
        "GEMINI_API_KEY": "@gemini_api_key",
        "ENVIRONMENT": "production"
    }
}